<p><b>Title:</b> <%= @test.title%></p>
<p><b>Level:</b> <%= @test.level%></p>
<p><b>Author:</b> <%= @test.author.email%></p>

<%= link_to 'Create Question', new_admin_test_question_path(@test) %>

<table>
  <tr>
    <th>Test</th>
    <th>Body</th>
    <th colspan="3">Actions</th>
  </tr>

  <%= render partial: 'admin/questions/question', collection: @test.questions %>

</table>

<%= link_to 'Edit', edit_admin_test_path(@test) %>
<%= link_to 'Back', admin_tests_path %>
